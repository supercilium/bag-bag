{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "products": {
      "via": "orders",
      "collection": "product",
      "dominant": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "paid",
        "processing",
        "delivering",
        "declined"
      ],
      "required": true,
      "default": "new"
    },
    "total": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "cash",
        "card"
      ],
      "required": true,
      "default": "card"
    },
    "promocode": {
      "model": "promocode",
      "via": "orders"
    },
    "user": {
      "private": true,
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    }
  }
}